<!DOCTYPE html>
<html>
  <head>
    <title>NewPetsProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%if owner_signed_in?%>
    hola: <%=current_owner.email%>
    <%=link_to "salir", destroy_owner_session_path, method: :delete%>
    <%=link_to "Perfil",edit_owner_registration_path%>
    <%else%>
    <%=link_to "login", new_owner_session_path%>
    <%end%>
    <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
